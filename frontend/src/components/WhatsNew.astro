---
import Page from './Page.astro';
import Card from './Card.astro';

export interface Props {
	title: string;
	body: string;
	href: string;
	disabled: boolean;
}

const { href, title, body, disabled } = Astro.props as Props;

let output = "foo";
---

<Page title="What's new">
    <h3>September 27, 2022</h3>
    <br />
    <p>You can now make a test print with your 3D printer.</p>

    <p>Added images for all <a href="/quests">quests</a> for all existing steps.</p>

    <p>Created the "What's new" page.</p>

</Page>

<style>
    a {
        opacity: 0.8;
		background-color: hsl(123, 100%, 22%);
		border-radius: 0.4rem;
		color: white;
		text-decoration: none;
		flex-direction: row;
		margin: 1px;
		padding: 5px;
		text-align: center;
    }

    p {
        background-color: #2f5b2f;
        padding: 15px;
        border-radius: 5px;
    }
</style>