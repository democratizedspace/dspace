---
import Page from '../components/Page.astro';
import Card from '../components/Card.astro';
import { parseBool, getCookieValue } from '../utils';

Astro.response.headers.append('Set-Cookie', 'unboxed3DPrinter=true; expires=Fri, 31 Dec 9999 23:59:59 GMT');

const cookie = Astro.request.headers.get('cookie');
const acceptedCookies = parseBool(getCookieValue(cookie, 'acceptedCookies'));
---

<Page message="You removed all the items and placed them on the table.">
    <Card
    href="/inventory"
    title="Check your inventory"
    body=""
    textbox={false}
    disabled={false}
    hazard={false}
    />
    <Card
    href="/3dprinter-removed-pieces"
    title="Check the manual"
    body=""
    textbox={false}
    disabled={true}
    hazard={false}
    />
</Page>